{
  "name": "sage",
  "version": "1.0.0",
  "description": "O Projeto SAGE cria novos blog posts com base nos posts dos concorrentes e os posta automaticamente em um outro blog.",
  "main": "index.js",
  "scripts": {
    "dev": "tsx src/run.ts",
    "dev:scrapper": "tsx src/use-cases/scrapper/run-scrapper.ts",
    "dev:rewriter": "tsx src/use-cases/rewriter/run-rewriter.ts",
    "dev:scheduler": "tsx src/use-cases/scheduler/run-scheduler.ts",
    "dev:imager": "tsx src/use-cases/imager/run-imager.ts",
    "dev:all": "tsx src/run-sage.ts",
    "prod": "node build/run.js",
    "prod:scrapper": "node build/use-cases/scrapper/run-scrapper.js",
    "build": "tsup src --out-dir build",
    "docker": "sudo docker compose up -d",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rocketseat/eslint-config": "1.2.0",
    "@types/node": "20.3.1",
    "@vitest/coverage-v8": "0.32.4",
    "@vitest/ui": "0.32.4",
    "eslint": "8.43.0",
    "prisma": "5.1.1",
    "tsup": "7.1.0",
    "tsx": "3.12.7",
    "typescript": "5.1.3",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.32.2"
  },
  "dependencies": {
    "@prisma/client": "4.16.1",
    "@types/bcryptjs": "2.4.2",
    "axios": "1.4.0",
    "bcryptjs": "2.4.3",
    "dotenv": "16.3.1",
    "fastify": "4.18.0",
    "html-dom-parser": "4.0.0",
    "openai": "3.3.0",
    "puppeteer": "20.9.0",
    "sharp": "0.32.4",
    "zod": "3.21.4"
  }
}
